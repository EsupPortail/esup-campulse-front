{
  "name": "plana",
  "version": "1.1.0-beta",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:test": "vue-tsc --noEmit && NODE_ENV=development vite build --mode deploy_test",
    "build:preprod": "vue-tsc --noEmit && NODE_ENV=production vite build --mode deploy_pprd",
    "build:prod": "vue-tsc --noEmit && NODE_ENV=production vite build --mode deploy_prod",
    "lint": "eslint . --ext .vue,.ts,.js,.json --fix --ignore-path .gitignore",
    "opalint": "eslint . --ext .vue,.ts,.js,.json --fix --ignore-path .gitignore && echo '\\033[0;32mgropif\\033[0;32m'",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "test:unit:coverage": "vitest --environment jsdom --coverage",
    "test:unit:ui": "vitest --environment jsdom --ui",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "prepare": "husky"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.9",
    "@sentry/vue": "^7.99.0",
    "@vue-unistra/cas-authentication": "^2.1.3",
    "axios": "^1.6.7",
    "bootstrap-icons": "^1.11.3",
    "pinia": "^2.1.7",
    "quasar": "^2.14.3",
    "vue": "^3.4.15",
    "vue-i18n": "^9.9.1",
    "vue-matomo": "^4.2.0",
    "vue-router": "^4.2.5",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@babel/types": "^7.23.9",
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@pinia/testing": "^0.1.3",
    "@quasar/vite-plugin": "^1.6.0",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.16",
    "@types/zxcvbn": "^4.4.4",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/coverage-v8": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.1",
    "husky": "^9.0.10",
    "jsdom": "^24.0.0",
    "sass": "^1.70.0",
    "typescript": "^4.9.5",
    "vite": "^5.0.12",
    "vitest": "^1.2.2",
    "vue-tsc": "^1.8.27"
  }
}

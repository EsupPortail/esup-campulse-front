{
  "name": "plana",
  "version": "0.1.20230512",
  "scripts": {
    "dev": "vite",
    "build:test": "vue-tsc --noEmit && vite build --mode deploy_test",
    "build:preprod": "vue-tsc --noEmit && vite build --mode deploy_pprd",
    "build:prod": "vue-tsc --noEmit && vite build --mode deploy_prod",
    "lint": "eslint . --ext .vue,.ts,.js --fix --ignore-path .gitignore",
    "opalint": "eslint . --ext .vue,.ts,.js --fix --ignore-path .gitignore && echo '\\033[0;32mgropif\\033[0;32m'",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "test:unit:coverage": "vitest --environment jsdom --coverage",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.3",
    "@sentry/tracing": "^7.50.0",
    "@sentry/vue": "^7.50.0",
    "@vue-unistra/cas-authentication": "^2.1.0",
    "axios": "^1.4.0",
    "bootstrap-icons": "^1.10.5",
    "pinia": "^2.0.35",
    "quasar": "^2.11.10",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@pinia/testing": "^0.0.16",
    "@quasar/vite-plugin": "^1.3.1",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.2",
    "@types/zxcvbn": "^4.4.1",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vitest/coverage-c8": "^0.30.1",
    "@vitest/ui": "^0.30.1",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^21.1.1",
    "sass": "^1.62.1",
    "typescript": "^4.9.5",
    "vite": "^4.3.3",
    "vitest": "^0.30.1",
    "vue-tsc": "^1.6.1"
  }
}

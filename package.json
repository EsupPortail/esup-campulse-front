{
  "name": "front",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build:test": "vue-tsc --noEmit && vite build --mode deploy_test",
    "build:preprod": "vue-tsc --noEmit && vite build --mode deploy_pprd",
    "build:prod": "vue-tsc --noEmit && vite build --mode deploy_prod",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@quasar/extras": "^1.15.5",
    "@sentry/tracing": "^7.20.1",
    "@sentry/vue": "^7.20.1",
    "axios": "^1.1.3",
    "pinia": "^2.0.13",
    "quasar": "^2.10.0",
    "vue": "^3.2.33",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.2.3",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.27",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.20",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^19.0.0",
    "sass": "^1.32.12",
    "typescript": "~4.6.3",
    "vite": "^2.9.5",
    "vitest": "^0.23.4",
    "vue-tsc": "^0.34.7"
  }
}
